{
  "name": "videojs-thumbnail-sprite",
  "version": "0.1.1",
  "description": "Video.js plugin to display preview image of a video at the point of time when hovering on progress bar",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnest:main": "dist/index.es.js",
  "files": [
    "dist/**/*"
  ],
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "npx jest --ci --coverage",
    "test:watch": "npx jest  --watchAll --coverage --verbose",
    "build:watch": "npx rollup -c --watch",
    "build:prod": "npx rollup -c rollup.config.prod.js",
    "prepare:peers": "npx install-peers -f",
    "prepare": "yarn prepare:peers && yarn build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadenzah/videojs-thumbnail-sprite.git"
  },
  "keywords": [
    "video.js",
    "videojs",
    "videojs-plugin",
    "typescript",
    "videojs-thumbnail-sprite"
  ],
  "author": "cadenzah <cadenzah93@gmail.com> (https://github.com/cadenzah)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cadenzah/videojs-thumbnail-sprite/issues"
  },
  "homepage": "https://github.com/cadenzah/videojs-thumbnail-sprite#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@types/jest": "^26.0.9",
    "@types/sinon": "^9.0.4",
    "install-peers-cli": "^2.2.0",
    "jest": "^26.2.2",
    "rollup": "^2.23.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "sinon": "^9.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "video.js": "^8.10.0"
  },
  "peerDependencies": {
    "video.js": "^8.10.0"
  }
}
